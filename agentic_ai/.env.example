# SymptomSync Agentic AI - Environment Variables

# Application Settings
SYMPTOMSYNC_ENVIRONMENT=development
SYMPTOMSYNC_APP_NAME=SymptomSync Agentic AI
SYMPTOMSYNC_APP_VERSION=1.0.0
SYMPTOMSYNC_DEBUG=false
SYMPTOMSYNC_LOG_LEVEL=INFO

# LLM Provider Settings (set at least one)
SYMPTOMSYNC_OPENAI_API_KEY=your-openai-api-key-here
# SYMPTOMSYNC_ANTHROPIC_API_KEY=your-anthropic-api-key-here
# SYMPTOMSYNC_GOOGLE_AI_API_KEY=your-google-ai-api-key-here

# Model Configuration
SYMPTOMSYNC_PRIMARY_MODEL=gpt-4-turbo-preview
SYMPTOMSYNC_FALLBACK_MODEL=gpt-3.5-turbo
SYMPTOMSYNC_TEMPERATURE=0.7
SYMPTOMSYNC_MAX_TOKENS=2000

# Vector Store Settings
SYMPTOMSYNC_VECTOR_STORE_TYPE=chroma
SYMPTOMSYNC_CHROMA_PERSIST_DIRECTORY=./data/chroma
# SYMPTOMSYNC_PINECONE_API_KEY=your-pinecone-api-key
# SYMPTOMSYNC_PINECONE_ENVIRONMENT=your-pinecone-environment
# SYMPTOMSYNC_PINECONE_INDEX_NAME=symptomsync

# Database Settings
SYMPTOMSYNC_DATABASE_URL=postgresql://user:password@localhost:5432/symptomsync

# Redis Settings
SYMPTOMSYNC_REDIS_HOST=localhost
SYMPTOMSYNC_REDIS_PORT=6379
SYMPTOMSYNC_REDIS_DB=0
# SYMPTOMSYNC_REDIS_PASSWORD=your-redis-password

# AWS Settings
SYMPTOMSYNC_AWS_REGION=us-east-1
# SYMPTOMSYNC_AWS_ACCESS_KEY_ID=your-aws-access-key
# SYMPTOMSYNC_AWS_SECRET_ACCESS_KEY=your-aws-secret-key
# SYMPTOMSYNC_AWS_S3_BUCKET=your-s3-bucket

# Azure Settings
# SYMPTOMSYNC_AZURE_SUBSCRIPTION_ID=your-subscription-id
# SYMPTOMSYNC_AZURE_RESOURCE_GROUP=your-resource-group
# SYMPTOMSYNC_AZURE_STORAGE_ACCOUNT=your-storage-account
# SYMPTOMSYNC_AZURE_STORAGE_KEY=your-storage-key

# MCP Server Settings
SYMPTOMSYNC_MCP_SERVER_HOST=0.0.0.0
SYMPTOMSYNC_MCP_SERVER_PORT=8000
SYMPTOMSYNC_MCP_WORKERS=4

# Agent Settings
SYMPTOMSYNC_MAX_AGENT_ITERATIONS=10
SYMPTOMSYNC_AGENT_TIMEOUT=300

# Monitoring Settings
SYMPTOMSYNC_ENABLE_METRICS=true
SYMPTOMSYNC_ENABLE_TRACING=true

# Rate Limiting
SYMPTOMSYNC_RATE_LIMIT_PER_MINUTE=60
